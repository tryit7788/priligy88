{
  "name": "astro-payloadcms-ecom",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "generate:types": "cd apps/payload_app && pnpm run generate:types"
  },
  "devDependencies": {
    "turbo": "^2.5.5"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "dependencies": {
    "marked": "^16.2.1",
    "pg": "^8.16.3",
    "puppeteer": "^24.19.0"
  }
}
